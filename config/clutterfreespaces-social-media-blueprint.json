      "metadata": {
        "designer": {
          "x": 300,
          "y": 0
        }
      }
    },
    {
      "id": 3,
      "module": "util:SetVariables",
      "version": 1,
      "parameters": {},
      "mapper": {
        "variables": [
          {
            "name": "weekNumber",
            "value": "{{floor(divide(now; 604800000))}}"
          },
          {
            "name": "selectedWeek",
            "value": "{{mod(weekNumber; 4) + 1}}"
          },
          {
            "name": "currentPost",
            "value": "{{get(filter(2.array; theme = if(selectedWeek = 1; \"RV Organization Tips\"; if(selectedWeek = 2; \"RV Organization Tips\"; if(selectedWeek = 3; \"Before & After Transformations\"; \"Organization Psychology\")))); 1)}}"
          }
        ],
        "scope": "roundtrip"
      },
      "metadata": {
        "designer": {
          "x": 600,
          "y": 0
        }
      }
    },
    {
      "id": 4,
      "module": "facebook:createPagePost",
      "version": 1,
      "parameters": {
        "page": "YOUR_FACEBOOK_PAGE_ID"
      },
      "mapper": {
        "message": "{{3.currentPost.post}}\\n\\n{{3.currentPost.hashtags}}",
        "link": "{{3.currentPost.link}}",
        "published": true
      },
      "metadata": {
        "designer": {
          "x": 900,
          "y": -100
        },
        "restore": {
          "page": {
            "label": "ClutterFreeSpaces Page"
          }
        }
      }
    },
    {
      "id": 5,
      "module": "instagram-for-business:createPost",
      "version": 1,
      "parameters": {
        "account": "YOUR_INSTAGRAM_BUSINESS_ID"
      },
      "mapper": {
        "caption": "{{3.currentPost.post}}\\n\\n{{3.currentPost.hashtags}}\\n\\nüîó Link in bio for quiz and consultation!",
        "mediaType": "TEXT"
      },
      "metadata": {
        "designer": {
          "x": 900,
          "y": 100
        },
        "restore": {
          "account": {
            "label": "ClutterFreeSpaces Instagram"
          }
        }
      }
    },
    {
      "id": 6,
      "module": "airtable2:ActionCreateRecord",
      "version": 3,
      "parameters": {
        "connection": "airtable2",
        "base": "appctzgHffSFd4Nq7",
        "table": "Analytics",
        "useColumnId": false
      },
      "mapper": {
        "Date": "{{formatDate(now; \"YYYY-MM-DD\")}}",
        "Lead Sources": "Social Media Post: {{3.currentPost.theme}}",
        "Notes": "Posted: {{truncate(3.currentPost.post; 100)}}... | FB Post ID: {{4.id}} | IG Post: {{5.id}}",
        "New Leads": 0
      },
      "metadata": {
        "designer": {
          "x": 1200,
          "y": 0
        },
        "restore": {
          "base": {
            "label": "ClutterFreeSpaces CRM"
          },
          "table": {
            "label": "Analytics"
          }
        }
      }
    }
  ],
  "metadata": {
    "version": 1,
    "scenario": {
      "roundtrips": 1,
      "maxErrors": 3,
      "autoCommit": false,
      "autoCommitTriggerLast": true,
      "sequential": false,
      "confidential": false,
      "dataloss": false,
      "dlq": false,
      "freshVariables": false
    },
    "designer": {
      "orphans": []
    },
    "zone": "eu1.make.com"
  },
  "notes": [
    {
      "id": 1,
