{
  "name": "ClutterFreeSpaces - Weekly Social Media Automation",
  "flow": [
    {
      "id": 1,
      "module": "webhook:webhook",
      "version": 1,
      "parameters": {
        "hook": 12345,
        "maxResults": 1
      },
      "mapper": {},
      "metadata": {
        "designer": {
          "x": 0,
          "y": 0
        },
        "restore": {},
        "parameters": [
          {
            "name": "hook",
            "type": "hook",
            "label": "Webhook"
          }
        ],
        "interface": [
          {
            "name": "trigger",
            "type": "text"
          },
          {
            "name": "timestamp",
            "type": "text"
          },
          {
            "name": "theme",
            "type": "text"
          },
          {
            "name": "source",
            "type": "text"
          }
        ]
      }
    },
    {
